<form method="post" action="{{ path('season_delete', {'id': season.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir effacer cette saison ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ season.id) }}">
    <button class="btn">Effacer</button>
</form>
