<form method="post" action="{{ path('program_delete', {'id': program.id}) }}" onsubmit="return confirm('Êtes vous sûr de voulour effecer cet Item ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ program.id) }}">
    <button class="btn">Effacer</button>
</form>
